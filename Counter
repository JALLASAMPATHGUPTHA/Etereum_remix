pragma solidity ^0.8.24;

contract Counter {
    uint256 public count;

    // Increment count
    function increment() external {
        count += 1;
    }

    // Decrement count
    function decrement() external {
        require(count > 0, "Count cannot go below zero");
        count -= 1;
    }
}
